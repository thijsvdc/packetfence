[tls-common]
certificate_profile=radius
dh_file = ${certdir}/dh
ca_path = ${cadir}
cipher_list = "DEFAULT"
ecdh_curve = "prime256v1"
ocsp=default

[tls-eap-fast]
certificate_profile=radius
dh_file = ${certdir}/dh
ca_path = ${cadir}
cipher_list = "ALL:!EXPORT:!eNULL:!SSLv2"
ecdh_curve = "prime256v1"
disable_tlsv1_2 = yes
ocsp=default
